{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@app/constants": [
                "./src/constants"
            ],
            "@app/components": [
                "./src/components" 
            ],
            "@app/features/*": [
                "./src/features/*"
            ],
            "@app/core": [
                "./src/core"
            ],
            "@app/assets": [
                "./src/assets"
            ],
            "@app/utils": [
                "./src/utils"
            ],
            "@app/api": [
                "./src/api"
            ],
            "@app/route": [
                "./src/route"
            ],
            "@app/store/*": [
                "./src/store/*"
            ]
        }
    }
}